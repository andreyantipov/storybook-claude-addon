{
  "name": "@andreyantipov/storybook-claude-addon",
  "version": "1.0.0",
  "description": "Storybook addon for Claude AI integration with xterm.js terminal and streaming",
  "license": "MIT",
  "author": "Andrey Antipov",
  "repository": {
    "type": "git",
    "url": "https://github.com/andreyantipov/storybook-claude-addon.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "claude-server": "./bin/claude-server.js"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "server": "node ./bin/claude-server.js",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./manager": {
      "types": "./dist/manager.d.ts",
      "import": "./dist/manager.mjs",
      "require": "./dist/manager.js"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "import": "./dist/preset.mjs",
      "require": "./dist/preset.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "bin/**/*",
    "README.md"
  ],
  "keywords": [
    "storybook-addons",
    "storybook",
    "claude",
    "anthropic",
    "ai",
    "xterm",
    "terminal",
    "streaming"
  ],
  "peerDependencies": {
    "@storybook/components": "^8.0.0",
    "@storybook/manager-api": "^8.0.0",
    "@storybook/theming": "^8.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@storybook/components": "^8.6.0",
    "@storybook/manager-api": "^8.6.0",
    "@storybook/theming": "^8.6.0",
    "@storybook/types": "^8.6.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^4.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.3.0"
  },
  "storybook": {
    "displayName": "Claude AI Assistant",
    "supportedFrameworks": ["react"],
    "icon": "https://www.anthropic.com/favicon.ico"
  }
}
